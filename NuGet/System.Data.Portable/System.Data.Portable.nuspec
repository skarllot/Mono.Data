<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
    <metadata>
        <id>System.Data.Portable</id>
        <version>1.0.3.5</version>
        <title>Mono.Data Plugin for Xamarin and Windows</title>
        <authors>Matthew Leibowitz, Fabrício Godoy</authors>
		<owners>mattleibow, fgodoy-verion</owners>
        <licenseUrl>https://github.com/fgodoy-verion/Mono.Data/blob/master/LICENSE.TXT</licenseUrl>
        <projectUrl>https://github.com/fgodoy-verion/Mono.Data</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Add Mono.Data to any Xamarin or Windows .NET app. 
Supports Xamarin.Android, Xamarin.iOS, Windows 8, Windows Desktop and Windows Phone 8</description>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="System.Data" targetFramework="net4" />
            <frameworkAssembly assemblyName="System.Data" targetFramework="MonoAndroid" />
            <frameworkAssembly assemblyName="System.Data" targetFramework="MonoTouch" />
            <frameworkAssembly assemblyName="System.Data" targetFramework="XamariniOS" />
		    <frameworkAssembly assemblyName="System.Transactions" targetFramework="net4" />
            <frameworkAssembly assemblyName="System.Transactions" targetFramework="MonoAndroid" />
            <frameworkAssembly assemblyName="System.Transactions" targetFramework="MonoTouch" />
            <frameworkAssembly assemblyName="System.Transactions" targetFramework="XamariniOS" />
        </frameworkAssemblies>
    </metadata>
</package>